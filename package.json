{
  "name": "authcov",
  "version": "1.1.3",
  "description": "Web app authorization coverage scanning using Chrome headless browser",
  "keywords": [
    "puppeteer",
    "security",
    "crawler",
    "scanner"
  ],
  "homepage": "https://github.com/authcov/authcov",
  "scripts": {
    "test": "jest --detectOpenHandles",
    "build": "tsc",
    "build:watch": "tsc --watch"
  },
  "bin": {
    "authcov": "bin/authcov"
  },
  "jest": {
    "transform": {
      ".(ts|tsx)": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "testTimeout": 30000
  },
  "author": "evan@evanrolfe.me",
  "license": "GPL-3.0",
  "type": "module",
  "dependencies": {
    "axios": "^0.24.0",
    "chalk": "^2.4.2",
    "commander": "^8.3.0",
    "ejs": "^3.1.6",
    "events": "^3.0.0",
    "lodash": "^4.17.13",
    "path": "^0.12.7",
    "puppeteer": "^7.0.0",
    "url": "^0.11.0",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@types/jest": "^27.0.3",
    "@types/node": "^17.0.0",
    "jest": "^27.4.5",
    "ts-jest": "^27.1.2",
    "typescript": "^4.5.4"
  }
}
